{
  "manifest_version": 2,

  "name": "private quick extension",
  "description": "only in developer mode",
  "version": "1.0",

  "page_action": {
    "default_icon": "icon.png"
  },

  "chrome_url_overrides" : {
    "newtab": "window.html"
  },

  "background": {
    "scripts": ["newtab.js"],

    "persistant": false
  },

  "offline_enabled": true,

  "permissions": [
    "tabs",
    
    "fileSystem",
    {"fileSystem": ["write"]},
    {"fileSystem": ["write", "retainEntries", "directory"]} 
  ]
}